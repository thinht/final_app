
<% if user_signed_in? %>

  <%= link_to t('.list_user'), users_path %><br>
  <%= link_to t('.current_user'), user_path(current_user) %><br>
  <%= link_to t('.sign_out'),  destroy_user_session_path, method: 'delete' %>

<% else %>

  <%= link_to t('.sign_in'),  user_session_path %><br>
  <%= link_to t('.sign_up'), new_user_registration_path %><br>

<% end %>

